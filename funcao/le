////////////////



function sorteio(minimo=0, maximo=10){
    let aleatorio = Math.floor(Math.random() * (maximo - minimo + 1)) + minimo;
    return aleatorio;
}


let numero1 = sorteio();
let numero2 = sorteio(0,50);
let numero3 = sorteio(15,35); 

console.log(numero1, numero2, numero3);


